{% extends 'base.html.twig' %}

{% block title %}Eleve index{% endblock %}

{% block body %}
    <h1>Eleve index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Matricule</th>
                <th>Username</th>
                <th>Prenom</th>
                <th>Nom</th>
                <th>Adresse</th>
                <th>NomcompletTuteur</th>
                <th>TelephoneTuteur</th>
                <th>EmailTuteur</th>
                <th>CreatedAt</th>
                <th>UpdatedAt</th>
                <th>Photo</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for eleve in eleves %}
            <tr>
                <td>{{ eleve.id }}</td>
                <td>{{ eleve.matricule }}</td>
                <td>{{ eleve.username }}</td>
                <td>{{ eleve.prenom }}</td>
                <td>{{ eleve.nom }}</td>
                <td>{{ eleve.adresse }}</td>
                <td>{{ eleve.nomcompletTuteur }}</td>
                <td>{{ eleve.telephoneTuteur }}</td>
                <td>{{ eleve.emailTuteur }}</td>
                <td>{{ eleve.createdAt ? eleve.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ eleve.updatedAt ? eleve.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ eleve.photo }}</td>
                <td>
                    <a href="{{ path('app_eleve_show', {'id': eleve.id}) }}">show</a>
                    <a href="{{ path('app_eleve_edit', {'id': eleve.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="13">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_eleve_new') }}">Create new</a>
{% endblock %}
